@model List<Channel>
 
<div id="sidebar">
    <ul class="sidebar-menu">
        <li class="toggle-sidebar"><a href="#" data-toggle="sidebar"><i class="fas fa-users"></i></a></li>
        @foreach(var channel in Model)
        {
                     
            <li>
                <a href="/channel/@channel.Id" class="current-server" data-toggle="tooltip" data-placement="right" data-title="@channel.Name">
                    <img src="@channel.Picture" alt="">
                </a>
            </li>
        } 
        <li><a href="#" 
               class="join-server" 
               data-start="modal-custom" 
               data-target="#create-group" 
               data-toggle="tooltip" 
               data-placement="right" 
               data-title="Join a Group">
                <i class="fa fa-plus"></i>
            </a>
        </li>
    </ul>
</div>